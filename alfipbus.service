[Unit]
Description=ALFIPbus
After=network.target
[Install]
WantedBy=multi-user.target
[Service]
Type=exec
ExecStart=/usr/local/bin/AlfIPbus -n ${NAME} -l ${DEVICE_ADDRESS} -t ${TIMEOUT} -f ${LOG_FILE} ${FLAGS}
Restart=on-failure
RestartSec=10
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=%n
